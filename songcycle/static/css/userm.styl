.user-m-create-area{
    padding-top: 4rem;
    padding-bottom: 2rem;
    padding-right: 15rem;
    padding-left: 15rem;
    color: #212529;

    .user-m-file{
        padding-right: 3rem;
    }

    .file-name-label{
        padding-top:0.3rem;
    }

    .user-m-create-title{
        padding-bottom:4rem;
        text-align: left;
    }

    .continue-to-register-none{
        display:none;
    }

    .continue-to-register{
        text-align: right;
    }
}

.cover-user-m{
    padding:2rem;

    .search-sec {
        padding-bottom: 2rem;
        padding-left: 3.0rem;

        .search-email{
            width: 300px;
        }

        .search-full-name{
            width: 300px;
        }
    }

    .user-m-create-link{
        float: right;
        padding-right:2.8rem;
        padding-bottom: 2rem;
    }

    .user-m-table-area{
        max-height:45%;
        padding-right:2rem;
        padding-left:2rem;
        padding-bottom: 2rem;

        .sort-header:hover{
            color:#4169e1;
        }

        .target-year{
            width:50px;
        }

        .auther-user{
            width:100px;
        }

        .file-name{
            width:400px;
        }

        .down-load-count{
            width:100px;
        }

        .request-download{
            width:100px;
        }

        .delete-user-m{
            width:100px;
        }
    }

    .user-m-pagination{
        float: right;
        padding-top:1rem;
        padding-right:2.8rem;
    }
}